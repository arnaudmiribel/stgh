{
    "manifest_version": 2,
    "name": "Streamlit >< GitHub toggle",
    "version": "0.1.0",
    "description": "Switch from Streamlit to GitHub and vice versa!",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "favicon-16x16.png",
        "32": "favicon-32x32.png",
        "128": "icon.png"
    }
}